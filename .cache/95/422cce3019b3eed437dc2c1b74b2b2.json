'Roboto', sans-serif;\n}\n","scss/components/_nav.scss":"@use '../abstracts' as *;\nnav {\n  position: absolute;\n  text-align: left;\n  top: 100%;\n  left: 0;\n  background: var(--clr-primary-400);\n  width: 100%;\n  transform-origin: top;\n  transform: scale(1, 0);\n  transition: transform 200ms ease-in-out;\n  ul {\n    margin: 0;\n    padding: 0;\n    list-style: none;\n\n    @include mq(medium) {\n      display: flex;\n      justify-content: flex-end;\n    }\n  }\n  li {\n    margin-bottom: 0.75em;\n    margin-left: 0.75em;\n\n    @include mq(medium) {\n      margin-left: 2.75em;\n      margin-bottom: 0;\n    }\n  }\n  a {\n    text-decoration: none;\n    color: var(--clr-mono-200);\n    font-size: 1.125rem;\n    text-transform: uppercase;\n    opacity: 0;\n    transition: opacity 60ms ease-in-out;\n\n    &:hover {\n      color: var(--clr-mono-600);\n    }\n\n    @include mq(medium) {\n      opacity: 1;\n      position: relative;\n      &:hover {\n        color: var(--clr-mono-200);\n        &::before {\n          transform: scale(1, 1);\n        }\n      }\n      &::before {\n        content: '';\n        display: block;\n        height: 4px;\n        background: var(--clr-mono-400);\n        position: absolute;\n        top: -0.5em;\n        left: 0;\n        right: 0;\n        transform: scale(0, 1);\n        transition: transform ease-in-out 233ms;\n      }\n    }\n  }\n\n  @include mq(medium) {\n    all: unset;\n    grid-column: 3 / span 1;\n    display: flex;\n    justify-content: flex-end;\n    align-items: center;\n  }\n}\n","scss/abstracts/_breakpoints.scss":"@use 'sass:map';\n$breakpoints: (\n  small: 43em,\n  medium: 62em,\n  large: 82em,\n);\n\n@mixin mq($key) {\n  $size: map.get($breakpoints, $key);\n\n  @media screen and (min-width: $size) {\n    @content;\n  }\n}\n\n","scss/components/_header.scss":"@use '../abstracts' as *;\n@use 'sass:map';\nheader {\n  background: var(--clr-primary-400);\n  text-align: center;\n  position: fixed;\n  z-index: 999;\n  width: 100%;\n  font-family: var(--font-primary);\n  color: var(--clr-mono-200);\n  margin: 0;\n  padding: 0;\n\n  img {\n    display: none;\n  }\n\n  input[type='checkbox'] {\n    display: none;\n    &:checked ~ nav {\n      transform-origin: top;\n      transform: scale(1, 1);\n      transition: transform 200ms ease-in-out;\n      a {\n        opacity: 1;\n        transition: opacity 100ms ease-in-out 160ms;\n      }\n    }\n  }\n  label[for='nav-toggle'] {\n    position: absolute;\n    top: 0;\n    left: 0;\n    margin-left: 0.75em;\n    height: 100%;\n    display: flex;\n    align-items: center;\n    span,\n    span::after,\n    span::before {\n      display: block;\n      background: var(--clr-mono-200);\n      height: 2px;\n      width: 1.5em;\n      border-radius: 0.125em;\n      position: relative;\n    }\n    span::after,\n    span::before {\n      content: '';\n      position: absolute;\n    }\n\n    span::before {\n      bottom: 6px;\n    }\n    span::after {\n      top: 6px;\n    }\n\n    @include mq(medium) {\n      display: none;\n    }\n  }\n\n  h1 {\n    @include mq(medium) {\n      grid-column: 2 / span 1;\n    }\n  }\n\n  @include mq(medium) {\n    display: grid;\n    grid-template-columns: 1fr auto minmax(38em, 1fr) 1fr;\n    gap: 1em;\n    img {\n      height: 5em;\n      display: block;\n      grid-column: 1;\n      grid-row: 1;\n    }\n  }\n}\n","scss/main.scss":"@use 'abstracts';\n@use 'components';\n\nbody {\n  margin: 0;\n  padding: 0;\n}\n"},"lineCount":null}},"error":null,"hash":"a35daf17982c9cc56b77736e7ef27bf2","cacheData":{"env":{}}}