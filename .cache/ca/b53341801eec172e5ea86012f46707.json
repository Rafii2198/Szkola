'sass:map';\nheader {\n  background: var(--clr-primary-400);\n  text-align: center;\n  position: fixed;\n  z-index: 999;\n  width: 100%;\n  font-family: var(--font-primary);\n  color: var(--clr-mono-200);\n  margin: 0;\n  padding: 0;\n\n  img {\n    display: none;\n  }\n\n  input[type='checkbox'] {\n    display: none;\n    &:checked ~ nav {\n      transform-origin: top;\n      transform: scale(1, 1);\n      transition: transform 200ms ease-in-out;\n      a {\n        opacity: 1;\n        transition: opacity 100ms ease-in-out 160ms;\n      }\n    }\n  }\n  label[for='nav-toggle'] {\n    position: absolute;\n    top: 0;\n    left: 0;\n    margin-left: 0.75em;\n    height: 100%;\n    display: flex;\n    align-items: center;\n    span,\n    span::after,\n    span::before {\n      display: block;\n      background: var(--clr-mono-200);\n      height: 2px;\n      width: 1.5em;\n      border-radius: 0.125em;\n      position: relative;\n    }\n    span::after,\n    span::before {\n      content: '';\n      position: absolute;\n    }\n\n    span::before {\n      bottom: 6px;\n    }\n    span::after {\n      top: 6px;\n    }\n\n    @include mq(medium) {\n      display: none;\n    }\n  }\n\n  h1 {\n    @include mq(medium) {\n      grid-column: 2 / span 1;\n    }\n  }\n\n  @include mq(medium) {\n    display: grid;\n    grid-template-columns: 1fr auto minmax(38em, 1fr) 1fr;\n    gap: 1em;\n    img {\n      height: 5em;\n      display: block;\n      grid-column: 1;\n      grid-row: 1;\n    }\n  }\n}\n","scss/main.scss":"@use 'abstracts';\n@use 'components';\n\nbody {\n  margin: 0;\n  padding: 0;\n}\n"},"lineCount":null}},"error":null,"hash":"f9ad72a26d0ceebfb0bda26f6df410fd","cacheData":{"env":{}}}